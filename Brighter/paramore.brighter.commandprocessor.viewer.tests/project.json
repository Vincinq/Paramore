{
  "version": "1.0.0-*",
  "testRunner": "mspec",
  "dependencies": {
    "dotnet-test-nunit": "3.4.0-beta-1",
    "Machine.Specifications": "0.11.0",
    "Machine.Specifications.Should": "0.11.0",
    "Nancy.Testing": "2.0.0-barneyrubble",
    "NUnit": "3.4.0",
    "paramore.brighter.commandprocessor": {
      "target": "project"
    },
    "paramore.brighter.commandprocessor.messageviewer": {
      "target": "project"
    },
    "paramore.brighter.commandprocessor.tests": {
      "target": "project"
    }
  },
  "frameworks": {
    "netcoreapp1.0": {
      "imports": [
        "dnxcore50",
        "portable-net45+win8"
      ],
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.0.0"
        }
      }
    },
    "net452": {
      "dependencies": {
      }
    }
  },
  "tools": {
    "dotnet-test-mspec": "0.2.0"
  },
  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [ "DEBUG", "TRACE" ]
      }
    },
    "Release": {
      "buildOptions": {
        "define": [ "RELEASE", "TRACE" ],
        "optimize": true
      }
    }
  }
}
